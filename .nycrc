{
    "check-coverage": true,
    "per-file": true,
    "lines": 99,
    "statements": 99,
    "functions": 99,
    "branches": 99,
    "include": [
    "**/*.js"
    ],
    "exclude": [
    "test/**/*.js",
    "coverage/**",
    "report/**"
    ],
    "ignore-class-method": "methodToIgnore",
    "reporter": [
    "lcovonly",
    "cobertura",
    "html"
    ],
    "extension": [
    ".jsx"
    ],
    "cache": true,
    "all": true,
    "report-dir": "./report"
}